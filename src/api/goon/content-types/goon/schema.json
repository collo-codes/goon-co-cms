        "Female",
        "Other"
      ]
    },
    "bodyType": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "Slim",
        "Athletic",
        "Heavy",
        "Average"
      ]
    },
    "tools": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tool.tool",
      "mappedBy": "goon"
    },
    "socialMediaUrl": {
      "type": "string"
    },
    "bio": {
      "type": "text",
      "required": true
    },
    "reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::review.review",
      "mappedBy": "goon"
    },
    "previousWorks": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::previous-work.previous-work",
